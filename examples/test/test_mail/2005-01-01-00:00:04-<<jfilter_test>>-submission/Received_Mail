..#include <iostream>
..using std::cout;
..using std::cin;
..using std::endl;
..
..const unsigned MAX_LINE = 300;
..
..int main ( int argc )
..{
..    char line [MAX_LINE+1];
..    int count = 0;
..
..    while ( true )
..    {
..        cin.getline ( line, MAX_LINE );
..	if ( cin.eof() ) break;
..	if ( ++ count % 2 == 0
..	     && line[0] != 'C' ) cout << "OOPS ";
..	cout << line << endl;
..    }
..
..    return 0;
..}
