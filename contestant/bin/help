#!/bin/sh -f
#
# Display help information.
#
# File:		help
# Author:	Bob Walton <walton@deas.harvard.edu>
# Date:		Sun Sep 10 15:46:13 EDT 2000
#
# The authors have placed this program in the public
# domain; they make no warranty and accept no liability
# for this program.
#
# RCS Info (may not be true date or author):
#
#   $Author: hc3 $
#   $Date: 2000/09/18 01:38:25 $
#   $RCSfile: help,v $
#   $Revision: 1.1 $

if test ! -r ~/.hpcm_contest/help
then
	echo cannot read ~/.hpcm_contest/help
	exit 1
end

case "$1" in
     "")
     		less ~/.hpcm_contest/help/help.help
    		;;
     *)
		less ~/.hpcm_contest/help/$1.help
		;;
esac

exit 0
