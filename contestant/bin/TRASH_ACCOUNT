#!/bin/sh

case `who am i | awk '{print $1}' | sed 's/.*!//'` in
	acm-cont)
		echo no no no 
		exit 1 ;;
	acm[0-9]*)
		;;
	*)
		echo no no no
		exit 1 ;;
esac
cd
for i in .[a-zA-Z]* *
do
	case $i in
	.snapshot*)
		;;
	*)
		rm -rf $i
	esac
done

echo rm done
ls -a
tar xf /home/acm-cont/contestant/lib/setup.tar
echo tar done
ls -a
