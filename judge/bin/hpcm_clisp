# RCS Info (may not be true date or author):
#
#   $Author: hc3 $
#   $Date: 2002/02/17 13:10:40 $
#   $RCSfile: hpcm_clisp,v $
#   $Revision: 1.2 $


# Judge's version locates and executes contestant's
# version.  -which must be handled specially because
# sandbox cannot execute judge's version.
#
d=`dirname $0`
case "$1" in
    -which )

	case "$0" in
	    /*)
		d=`dirname $d`
		d=`dirname $d`
		echo $d/contestant/bin/hcpm_clisp
		exit 0
		;;
	esac
	;;
esac

exec $d/../../contestant/bin/hpcm_clisp "$@"
